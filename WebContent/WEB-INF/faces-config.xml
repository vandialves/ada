<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	
	<navigation-rule>
		<from-view-id>/pages/private/avaliacao/avaliacaoEdit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>successavaliacao</from-outcome>
			<to-view-id>/pages/private/avaliacao/avaliacaoReview.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/private/avaliacao/filterRelatorio.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>relatorio.xhtml</from-outcome>
			<to-view-id>/pages/private/avaliacao/relatorio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

</faces-config>